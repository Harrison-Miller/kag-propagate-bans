{
	"compilerOptions": {
		"target": "es2020",
		"module": "commonjs",
		"sourceMap": true,
		"removeComments": true,
		"outDir": "dist",
		"rootDir": "src",
		"strict": true,
		"noImplicitAny": true,
		"strictNullChecks": true,
		"noImplicitThis": true,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true
	},
	"include": ["src"]
}
